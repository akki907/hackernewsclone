{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server/index.ts"
      }
    ]
  }
  